\begin_layout Enumerate
Obtain equation
\begin_inset Formula 
\[
y=C_{1}e^{3x}+C_{2}e^{-3x}
\]

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $C_{1}$
\end_inset

 and 
\begin_inset Formula $C_{2}$
\end_inset


\begin_inset Formula 
\[
y=2,\frac{dy}{dx}=-1,x=0
\]

\end_inset


\begin_inset Formula 
\begin{align*}
2 & =C_{1}e^{3\left(0\right)}+C_{2}e^{-3\left(0\right)}\\
C_{1}+C_{2} & =2\\
C_{2} & =2-C_{1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{d}{dx}\left[C_{1}e^{3x}+C_{2}e^{-3x}\right]\\
 & =\frac{d}{dx}\left[C_{1}e^{3x}\right]+\frac{d}{dx}\left[C_{2}e^{-3x}\right]\\
 & =C_{1}\cdot3e^{3x}+C_{2}\left(-3\right)e^{-3x}\\
 & =C_{1}\cdot3e^{3x}-3C_{2}e^{-3x}\\
-1 & =C_{1}\cdot3e^{0}-3C_{2}e^{0}\\
-1 & =3C_{1}-3C_{2}\\
-1 & =3C_{1}-3\left(2-C_{1}\right)\\
-1 & =3C_{1}-6+3C_{1}\\
5 & =6C_{1}\\
C_{1} & =\frac{5}{6}\\
C_{1} & =\frac{5}{6}
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Substitute in 
\begin_inset Formula $C_{2}=2-C_{1}$
\end_inset


\begin_inset Formula 
\begin{align*}
C_{2} & =2-\frac{5}{6}\\
 & =\frac{7}{6}
\end{align*}

\end_inset


\end_layout

\end_deeper
