\begin_layout Enumerate
Factorize
\begin_inset Formula 
\begin{align*}
r & =\frac{-b\pm\sqrt{b^{2}-4ac}}{2a}\\
 & =\frac{-\left(-2\right)\pm\sqrt{\left(-2\right)^{2}-4\left(4\right)\left(1\right)}}{2\left(4\right)}\\
 & =\frac{2\pm\sqrt{4-16}}{8}\\
 & =\frac{2\pm\sqrt{-12}}{8}\\
 & =\frac{2\pm\sqrt{-1}\sqrt{12}}{8}\\
 & =\frac{2\pm\sqrt{-1}\cdot2\sqrt{3}}{8}\\
 & =\frac{2\pm2i\sqrt{3}}{8}\\
 & =\frac{1\pm i\sqrt{3}}{4}\\
r & =\frac{1}{4}\pm\frac{\sqrt{3}}{4}i
\end{align*}

\end_inset


\end_layout

