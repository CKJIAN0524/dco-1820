\begin_layout Enumerate
Obtain equation
\begin_inset Formula 
\begin{align*}
y & =e^{-x}\left(C_{1}\sin3x+C_{2}\cos3x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $C_{1}$
\end_inset

 and 
\begin_inset Formula $C_{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
0 & =e^{-0}\left(C_{1}\sin0+C_{2}\cos0\right)\\
 & =C_{2}\\
C_{2} & =0
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{d}{dx}\left[e^{-x}\left(C_{1}\sin3x+C_{2}\cos3x\right)\right]\\
 & =\frac{d}{dx}\left[e^{-x}\right]\cdot\left(C_{1}\sin3x+C_{2}\cos3x\right)+e^{-x}\cdot\frac{d}{dx}\left[\left(C_{1}\sin3x+C_{2}\cos3x\right)\right]\\
 & =-e^{-x}\cdot\left(C_{1}\sin3x+C_{2}\cos3x\right)+e^{-x}\left(C_{1}\frac{d}{dx}\left[\sin3x\right]+C_{2}\frac{d}{dx}\left[\cos3x\right]\right)\\
 & =-e^{-x}\left(C_{1}\sin3x+C_{2}\cos3x\right)+e^{-x}\left(3C_{1}\cos3x-3C_{2}\sin3x\right)
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Substitute in the values given and 
\begin_inset Formula $C_{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
6 & =-e^{0}\left(C_{1}\sin0+0\cos0\right)+e^{0}\left(3C_{1}\cos0-0\sin0\right)\\
 & =0+3C_{1}\left(1\right)+0\\
6 & =3C_{1}\\
C_{1} & =2
\end{align*}

\end_inset


\end_layout

\end_deeper
