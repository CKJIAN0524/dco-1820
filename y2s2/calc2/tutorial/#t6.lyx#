\end_layout

\begin_layout Enumerate
Factorize
\begin_inset Formula 
\begin{align*}
r^{2}+14r+49 & =0\\
\left(r+7\right)\left(r+7\right) & =0\\
\left(r+7\right)^{2} & =0\\
r & =-7
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Obtain equation
\begin_inset Formula 
\begin{align*}
y & =e^{-7x}\left(C_{1}+C_{2}x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $C_{1}$
\end_inset

 and 
\begin_inset Formula $C_{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
-1 & =e^{-7\left(-4\right)}\left(C_{1}+C_{2}\left(-4\right)\right)\\
-1 & =e^{28}\left(C_{1}-4C_{2}\right)\\
-1 & =C_{1}e^{28}-4C_{2}e^{28}\\
C_{1}e^{28} & =-1+4C_{2}e^{28}\\
C_{1} & =-e^{-28}+4C_{2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y'' & =\frac{d}{dx}\left[-e^{-7x}\left(C_{1}+C_{2}x\right)\right]\\
 & =\frac{d}{dx}\left[-C_{1}e^{-7x}-C_{2}e^{-7x}x\right]\\
 & =-C_{1}\left(-7\right)e^{-7x}-C_{2}\left(-7\right)e^{-7x}x\\
 & =7C_{1}e^{-7x}+7C_{2}e^{-7x}\\
5 & =7C_{1}e^{-7\left(-4\right)}+7C_{2}e^{-7\left(-4\right)}\\
5 & =7C_{1}e^{28}+7C_{2}e^{28}\\
5 & =7e^{28}\left(-e^{-28}+4C_{2}\right)+7C_{2}e^{28}\\
 & =-7+28C_{2}e^{28}+7C_{2}e^{28}\\
12 & =35C_{2}e^{28}\\
C_{2} & =\frac{12}{35}e^{-28}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
C_{1} & =-e^{-28}+4\left(\frac{12}{35}e^{-28}\right)\\
 & =-e^{-28}+\frac{48}{35}e^{-28}\\
 & =\frac{13}{35}e^{-28}
\end{align*}

\end_inset


\end_layout

