\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\lt{\frac{2}{3}}\frac{6x^{2}-7x+2}{6x-4}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt{\frac{2}{3}}\frac{6x^{2}-7x+2}{6x-4} & =\lt{\frac{2}{3}}\frac{0}{0}
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Since we can't factor it out, we can use L'Hospital's rule
\begin_inset Formula 
\begin{align*}
\lt{\frac{2}{3}}\frac{6x^{2}-7x+2}{6x-4} & =\lt{\frac{2}{3}}\frac{\frac{d}{dx}\left[6x^{2}-7x+2\right]}{\frac{d}{dx}\left[6x-4\right]}\\
 & =\lt{\frac{2}{3}}\frac{12x-7}{6}\\
 & =\frac{12\left(\frac{2}{3}\right)-7}{6}\\
 & =\frac{8-7}{6}\\
\lt{\frac{2}{3}}\frac{6x^{2}-7x+2}{6x-4} & =\frac{1}{6}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\lt{\infty}\frac{x^{5}+4x+1}{-3x^{5}+6x^{3}+7}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt{\infty}\frac{x^{5}+4x+1}{-3x^{5}+6x^{3}+7} & =\lt{\infty}\frac{\frac{x^{5}}{x^{5}}+\frac{4x}{x^{5}}+\frac{1}{x^{5}}}{\frac{-3x^{5}}{x^{5}}+\frac{6x^{3}}{x^{5}}+\frac{7}{x^{5}}}\\
 & =\lt{\infty}\frac{1+\frac{4x}{x^{5}}+\frac{1}{x^{5}}}{-3+\frac{6x^{3}}{x^{5}}+\frac{7}{x^{5}}}\\
 & =\frac{1+0+0}{-3+0+0}\\
 & =-\frac{1}{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt{-1}\frac{\sqrt{x+5}-2}{x+1}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt{-1}\frac{\sqrt{x+5}-2}{x+1}\cdot\frac{\sqrt{x+5}+2}{\sqrt{x+5}+2} & =\lt{-1}\frac{x+5-2^{2}}{x+1\left(\sqrt{x+5}+2\right)}\\
 & =\lt{-1}\frac{\cancel{x+1}}{\cancel{x+1}\left(\sqrt{x+5}+2\right)}\\
 & =\lt{-1}\frac{1}{\sqrt{x+5}+2}\\
 & =\frac{1}{\sqrt{\left(-1\right)+5}+2}\\
 & =\frac{1}{\sqrt{4}+2}\\
 & =\frac{1}{2+2}\\
 & =\frac{1}{4}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $h\left(x\right)=\frac{2x-1}{x-3},x\neq3$
\end_inset

.
 
\begin_inset Formula $g\left(x\right)=2x^{2}-1$
\end_inset

.
 Find 
\begin_inset Formula $\left(h\circ g\right)\left(x\right)$
\end_inset

 and 
\begin_inset Formula $\left(h^{-1}\circ g\right)\left(x\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $\left(h\circ g\right)\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\left(h\circ g\right)\left(x\right) & =h\left(g\left(x\right)\right)\\
 & =h\left(2x^{2}-1\right)\\
 & =\frac{2\left(2x^{2}-1\right)-1}{\left(2x^{2}-1\right)-3}\\
 & =\frac{4x^{2}-2-1}{2x^{2}-1-3}\\
\left(h\circ g\right)\left(x\right) & =\frac{4x^{2}-3}{2x^{2}-4}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $\left(h^{-1}\circ g\right)\left(x\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $h^{-1}$
\end_inset

, Let 
\begin_inset Formula $y=h\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =\frac{2x-1}{x-3}\\
y\left(x-3\right) & =2x-1\\
xy-3y & =2x-1\\
-3y-1 & =2x-xy\\
-3y-1 & =x\left(2-y\right)\\
x & =\frac{-\left(3y+1\right)}{\left(2-y\right)}\\
h^{-1} & =-\frac{3x+1}{2-x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $\left(h^{-1}\circ g\right)\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\left(h^{-1}\circ g\right)\left(x\right) & =h^{-1}\left(g\left(x\right)\right)\\
 & =h^{-1}\left(2x^{2}-1\right)\\
 & =-\frac{3\left(2x^{2}-1\right)}{2-\left(2x^{2}-1\right)}\\
 & =-\frac{6x^{2}-3}{-2x^{2}+1+2}\\
 & =-\frac{6x^{2}-3}{3-2x^{2}}\\
 & =\frac{6x^{2}-3}{2x^{2}-3}
\end{align*}

\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Enumerate
Find value of 
\begin_inset Formula $a$
\end_inset

 if 
\begin_inset Formula $h\left(x\right)=\begin{cases}
2x-1 & 1<x<2\\
x+a & x\ge2
\end{cases}$
\end_inset

is continuos everywhere.
 
\end_layout

\begin_deeper
\begin_layout Enumerate
Since is continuous everywhere, 
\begin_inset Formula $\lt{2^{-}}h\left(x\right)=\lt{2^{+}}h\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt{2^{-}}h\left(x\right) & =\lt{2^{+}}h\left(x\right)\\
\lt{2^{-}}\left(2x-1\right) & =\lt{2^{+}}\left(x+a\right)\\
2\left(2\right)-1 & =2+a\\
a & =4-1-2\\
\boldsymbol{a} & \boldsymbol{=1}
\end{align*}

\end_inset


\end_layout

