\begin_layout Enumerate
\begin_inset Formula $f\left(x\right)=2\cos x+\sin^{2}x$
\end_inset


\begin_inset Formula 
\begin{align*}
f\left(x\right) & =2\cos x+\left(\sin x\right)^{2}\\
f'\left(x\right) & =2\left(-\sin x\right)+2\left(\sin x\right)\cdot\frac{d}{dx}\left(\sin x\right)\\
 & =-2\sin x+2\sin x\cdot\cos x\\
\boldsymbol{f'\left(x\right)} & =\boldsymbol{-2\sin x+\sin2x};\text{Note:}\text{2\ensuremath{\sin x\cos x=\sin2x}}\\
f''\left(x\right) & =-2\left(\cos x\right)+\left(\cos2x\right)\left(2\right)\\
\boldsymbol{f''\left(x\right)} & \boldsymbol{=2\left(\cos2x-\cos x\right)}
\end{align*}

\end_inset


\end_layout

