\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\tan^{2}\left(3\theta\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =\left(\tan3\theta\right)^{2}\\
 & =2\left(\tan3\theta\right)\cdot\frac{d}{dx}\left(\tan3\theta\right)\\
 & =2\left(\tan3\theta\right)\cdot\sec^{2}3\theta\cdot3\\
 & =6\tan3\theta\sec^{2}3\theta
\end{align*}

\end_inset


