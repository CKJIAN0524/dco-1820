\begin_body

\begin_layout Title
\begin_inset FormulaMacro
\newcommand{\lt}[3]{}
{\lim_{#1\to#2}\left(#3\right)}
\end_inset


\end_layout

\begin_layout Title
Calculus L2 - LIMITS AND CONTINUITY
\end_layout

\begin_layout Section
Example 2.1
\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\lim_{x\to2}5=5$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x2{4x-5}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt x2{4x-5} & =4\left(2\right)-5\\
 & =3
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x0{2x^{2}+3x-5}$
\end_inset


\begin_inset Formula 
\[
\lt x0{2x^{2}+3x-5}=-5
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x0{x+1}\left(x-1\right)$
\end_inset


\series bold

\begin_inset Formula 
\[
\lt x0{x+1}\left(x-1\right)=-1
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x2{\frac{x-1}{x^{2}-1}}$
\end_inset


\series bold

\begin_inset Formula 
\begin{align*}
\boldsymbol{\lt x2{\frac{x-1}{x^{2}-1}}} & \boldsymbol{=\frac{1}{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x1{\frac{x-1}{x^{2}-1}}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lt x2{\frac{x-1}{x^{2}-1}} & =\lt x1{\frac{x-1}{x^{2}-1}}\\
 & =\frac{0}{0}\left(indeterminant\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\lt x2{\frac{x-1}{x^{2}-1}} & =\lt x1{\frac{\cancel{x-1}}{\cancel{\left(x-1\right)}\left(x+1\right)}}\\
 & =\lt x1{\frac{1}{\left(x+1\right)}}\\
\boldsymbol{\lt x2{\frac{x-1}{x^{2}-1}}} & \boldsymbol{=\frac{1}{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x3{\frac{x^{2}-x-6`}{x-3}}$
\end_inset


\begin_inset Formula 
\[
\lt x3{\frac{x^{2}-x-6`}{x-3}}=\frac{0}{0}\left(indeterminant\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\lt x3{\frac{x^{2}-x-6`}{x-3}} & =\lt x3{\frac{\cancel{\left(x-3\right)}\left(x+2\right)}{\cancel{x-3}}}\\
 & =\lt x3{x+2}\\
 & =5
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lt x{-1}{\frac{x^{2}+4x+3}{x+1}}$
\end_inset


\begin_inset Formula 
\[
\lt x{-1}{\frac{x^{2}+4x+3}{x+1}}=\frac{0}{0}\left(indeterminate\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\lt x{-1}{\frac{x^{2}+4x+3}{x+1}} & =\lt x{-1}{\frac{\left(x+3\right)\cancel{\left(x+1\right)}}{\cancel{x+1}}}\\
 & =-1+3\\
 & =2
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate

\end_layout

\end_deeper
