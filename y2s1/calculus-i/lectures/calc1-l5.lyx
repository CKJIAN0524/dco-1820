\begin_layout Enumerate
\begin_inset Formula $\int\left(2+3x\right)^{15}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=2+3x$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =3\\
dx & =\frac{du}{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find the derivative with respect to 
\begin_inset Formula $u$
\end_inset


\begin_inset Formula 
\begin{align*}
\int u^{15}\frac{du}{3} & =\frac{1}{3}\int u^{15}du\\
 & =\frac{1}{3}\left(\frac{u^{16}}{16}\right)+c\\
 & =\frac{u^{16}}{48}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute 
\begin_inset Formula $2+3x$
\end_inset

 back in to replace 
\begin_inset Formula $u$
\end_inset

 
\begin_inset Formula 
\[
\int\left(2+3x\right)^{15}dx=\frac{\left(2+3x\right)^{16}}{48}+c
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\int2x\left(x^{2}+3\right)^{5}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=x^{2}+3$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =2x\\
dx & =\frac{du}{2x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find the derivative with respect to 
\begin_inset Formula $u$
\end_inset

 
\begin_inset Formula 
\begin{align*}
\int2x\left(x^{2}+3\right)^{5}dx & =\int u^{5}du\\
 & =\frac{u^{6}}{6}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula 
\[
\int2x\left(x^{2}+3\right)^{5}dx=\frac{1}{6}\left(x^{2}+3\right)^{6}+c
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=\left(x^{3}-x+4\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =3x^{2}-1\\
dx & =\frac{du}{3x^{2}-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute into question
\begin_inset Formula 
\begin{align*}
\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}} & =\int\left(3x^{2}-1\right)u^{-4}\frac{du}{\left(3x^{2}-1\right)}\\
 & =\int u^{-4}du\\
 & =\frac{u^{-3}}{-3}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=\left(x^{3}-x+4\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}} & =-\frac{1}{3\left(x^{3}-x+4\right)}+c\\
 & =\frac{1}{-3x^{2}+x-4}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\int e^{5x}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=5x$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =5x\\
\ds u & =5\\
dx & =\frac{du}{5}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute into equation
\begin_inset Formula 
\begin{align*}
\int e^{u}\frac{du}{5} & =\frac{1}{5}\int e^{u}du\\
 & =\frac{1}{5}\left[e^{u}\right]+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=5x$
\end_inset


\begin_inset Formula 
\[
\int e^{5x}dx=\frac{1}{5}e^{5x}+c
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\ix{2xe^{1-x^{2}}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=1-x^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =1-x^{2}\\
\ds u & =-2x\\
dx & =-\frac{du}{2x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute into the equation
\begin_inset Formula 
\begin{align*}
\int2xe^{u}\left(-\frac{du}{2x}\right) & =-\int e^{u}du\\
 & =-e^{u}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=1-x^{2}$
\end_inset


\begin_inset Formula 
\[
\int2xe^{u}\left(-\frac{du}{2x}\right)=-e^{1-x^{2}}+c
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\ix{\frac{x}{e^{4x^{2}}}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=4x^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =4x^{2}\\
\ds u & =8x\\
dx & =\frac{du}{8x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute into the equation
\begin_inset Formula 
\begin{align*}
\int\frac{x}{e^{u}}\frac{du}{8x} & =\int\frac{1}{8e^{u}}du\\
 & =\frac{1}{8}\int e^{-u}du\\
 & =\frac{1}{8}\left(-e^{-u}\right)+c\\
 & =-\frac{1}{8}e^{-u}+c\\
 & =-\frac{1}{8e^{u}}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=4x^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\frac{x}{e^{4x^{2}}}} & =-\frac{1}{8e^{4x^{2}}}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\ix{\frac{2x}{x^{2}+3}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=x^{2}+3$
\end_inset


\begin_inset Formula 
\begin{align*}
\ds u & =2x\\
dx & =\frac{du}{2x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute inside the question
\begin_inset Formula 
\begin{align*}
\ix{\frac{2x}{x^{2}+3}} & =\int\frac{2x}{u}\frac{du}{2x}\\
 & =\int\frac{1}{u}du\\
 & =\ln\left|u\right|+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=x^{2}+3$
\end_inset


\begin_inset Formula 
\[
\ix{\frac{2x}{x^{2}+3}}=\ln\left|x^{2}+3\right|+c
\]

\end_inset


\end_layout

\end_deeper
