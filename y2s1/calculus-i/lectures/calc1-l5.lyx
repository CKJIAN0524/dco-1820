\begin_layout Enumerate
\begin_inset Formula $\ix{\cos3x\cos x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\cos3x\cos x} & =\int\frac{1}{2}\left[\cos\left(3x+x\right)+\cos\left(3x-x\right)\right]\\
 & =\frac{1}{2}\ix{\left[\cos\left(4x\right)+\cos\left(2x\right)\right]}\\
 & =\frac{1}{2}\left(\frac{1}{4}\sin\left(4x\right)+\frac{1}{2}\sin\left(2x\right)\right)+c\\
 & =\frac{1}{8}\sin4x+\frac{1}{4}\sin2x+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\int\sin3x\cos x\,dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\int\sin3x\cos x\,dx & =\ix{\frac{1}{2}\left[\sin\left(3x+x\right)+\sin\left(3x-x\right)\right]}\\
 & =\frac{1}{2}\int\left(\sin\left(4x\right)+\sin\left(2x\right)\right)dx\\
 & =\frac{1}{2}\left[-\frac{1}{4}\cos4x-\frac{\cos2x}{2}\right]+c\\
 & =-\frac{1}{8}\cos4x-\frac{1}{4}\cos2x+c
\end{align*}

\end_inset


\end_layout

