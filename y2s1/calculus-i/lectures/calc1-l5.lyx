\begin_layout Enumerate
\begin_inset Formula $\ix{\sin^{2}x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\sin^{2}x} & =\int\frac{1-\cos2x}{2}\\
 & =\frac{1}{2}\left[x-\frac{1}{2}\sin2x\right]+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\ix{\cos^{2}x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\cos^{2}x} & =\ix{\frac{\cos2x+1}{2}}\\
 & =\frac{1}{2}\ix{\cos2x+1}\\
 & =\frac{1}{2}\left[\frac{1}{2}\sin2x+x\right]+c\\
 & =\frac{1}{4}\sin2x+\frac{1}{2}x+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\ix{\tan^{2}x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\tan^{2}x} & =\int\left(\sec^{2}x-1\right)dx\\
 & =\tan x-x+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\ix{\cot^{2}x}$
\end_inset


\begin_inset Formula 
\[
\ix{\csc^{2}x+1}=-\cot x+x+c
\]

\end_inset


\end_layout

