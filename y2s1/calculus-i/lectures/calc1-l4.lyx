\begin_layout Enumerate
\begin_inset Formula $x^{3}+2y^{3}+3xy=0$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
3x^{2}+6y^{2}\dy+3\left(\dd\left[x\right]\cdot y+x\dd\left[y\right]\right) & =\dd\left[0\right]\\
3x^{2}+6y^{2}\dy+3\left(1\cdot y+x\dy\right) & =0\\
3x^{2}+6y^{2}\dy+3y+3x\dy & =0\\
3x^{2}+3y & =-\left(6y^{2}\dy+3x\dy\right)\\
 & =-\dy\left(6y^{2}+3x\right)\\
-\left(3x^{2}+3y\right) & =\dy\left(6y^{2}+3x\right)\\
\dy & =-\frac{\left(3x^{2}+3y\right)}{\left(6y^{2}+3x\right)}\\
 & =-\frac{3x^{2}+3y}{3x+6y^{2}}\\
 & =-\frac{3\left(x^{2}+y\right)}{3\left(x+2y^{2}\right)}\\
 & =-\frac{x^{2}+y}{x+2y^{2}}
\end{align*}

\end_inset


\end_layout

