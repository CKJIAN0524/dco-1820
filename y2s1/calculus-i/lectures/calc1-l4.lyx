\begin_layout Standard
\begin_inset Formula $x^{2}y+xy^{2}=12,\left(1,3\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\dd\left[x^{2}y+xy^{2}\right] & =\dd\left[12\right]\\
\frac{d}{dx}\left[x^{2}\right]\cdot y+x^{2}\dy+y^{2}+x\dy\cdot2y+x\cdot\dd\left[y^{2}\right] & =0\\
2x\cdot y+x^{2}\dy+y^{2}+2xy\dy & =0\\
2xy+y^{2} & =-2xy\dy-x^{2}\dy\\
\dy & =\frac{2xy+y^{2}}{-2xy-x^{2}}\\
\dy & =\frac{2\left(1\right)\left(3\right)+\left(3\right)^{2}}{-2\left(1\right)\left(3\right)-\left(1\right)^{2}}\\
\dy & =\frac{6+9}{-6-1}\\
 & =-\frac{15}{7}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y-y_{1} & =m\left(x-x_{1}\right)\\
y-3 & =-\frac{15}{7}\left(x-1\right)\\
y & =-\frac{15}{7}x+\frac{15}{7}+3\\
\boldsymbol{y} & \boldsymbol{=-\frac{15}{7}x+\frac{36}{7}}\\
7y & =-15x+36\\
7y+15x & =36
\end{align*}

