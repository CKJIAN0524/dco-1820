\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=\left(2x-3\right)^{5}$
\end_inset


\begin_inset Formula 
\[
u=2x-3
\]

\end_inset


\begin_inset Formula 
\[
y=u^{5}
\]

\end_inset


\begin_inset Formula 
\[
\frac{dy}{dx}=2
\]

\end_inset


\begin_inset Formula 
\[
\frac{dv}{dx}=5u^{4}\cdot\frac{dy}{dx}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =5\left(2x-3\right)^{4}\left(2\right)\\
\frac{dy}{dx} & =10\left(2x-3\right)^{4}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\ln\left(2x-x^{5}\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =2x-x^{5}\\
\frac{du}{dx} & =2-5x^{4}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y & =\ln\left(u\right)\\
\frac{dy}{dx} & =\frac{1}{u}\cdot\frac{du}{dx}\\
 & =\frac{2-5x^{4}}{2x-x^{5}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\sin\left(7x^{2}\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =7x^{2}\\
\frac{du}{dx} & =14x
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y & =\sin u\\
\frac{dy}{dx} & =\cos u\\
\boldsymbol{\frac{dy}{dx}} & \boldsymbol{=\cos\left(14x\right)}
\end{align*}

\end_inset


\end_layout

