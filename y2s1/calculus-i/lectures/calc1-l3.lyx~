\begin_layout Enumerate
\begin_inset Formula $y=-7e^{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =-7e^{x}\\
\frac{dy}{dx} & =-7*1e^{x}\\
 & =-7e^{x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{1}{2}cos\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =\frac{1}{2}\left(-sin\left(x\right)\right)\\
 & =-\frac{1}{2}sin\left(x\right)
\end{align*}

\end_inset


\end_layout

