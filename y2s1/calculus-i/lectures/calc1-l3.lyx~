\begin_layout Enumerate
\begin_inset Formula $y=2x^{4}+\frac{3}{x^{2}}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\dx\left(2x^{4}\right)+\dx\left(\frac{3}{x^{2}}\right)\\
 & =8x^{3}-\frac{6}{x^{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=e^{x}-5sin\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\dx\left(e^{x}\right)-5\dx\left(sin\left(x\right)\right)\\
 & =e^{x}-5\left(cos\left(x\right)\right)\\
 & =e^{x}-5cos\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{x^{3}+4}{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\dx\left(x^{2}\right)+\dx\left(\frac{4}{x}\right)\\
 & =2x-\frac{4}{x^{2}}
\end{align*}

\end_inset


\end_layout

\end_deeper
