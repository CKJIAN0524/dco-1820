\begin_body

\begin_layout Title
Calculus 1 C3: Differentiation
\end_layout

\begin_layout Standard
\begin_inset FormulaMacro
\newcommand{\dx}{\frac{dy}{dx}}
{\frac{dy}{dx}}
\end_inset


\end_layout

\begin_layout Section
Example
\end_layout

\begin_layout Enumerate
\begin_inset Formula $10x$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(h\right)}{h}\\
 & =\lim_{h\to0}\frac{10\left(x+h\right)-10x}{h}\\
 & =\lim_{h\to0}\frac{\cancel{10x}+10h\cancel{-10x}}{h}\\
 & =\lim_{h\to0}\frac{+10h}{h}\\
f'\left(x\right) & =10
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $3x^{2}+x+1$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(h\right)}{h}\\
 & =\lim_{h\to0}\frac{3\left(x+h\right)^{2}+x+h+1-3x^{2}+x+1}{h}\\
 & =\lim_{h\to0}\frac{3\left(x^{2}+2hx+h^{2}\right)+x+h+1-3x^{2}+x+1}{h}\\
 & =\lim_{h\to0}\frac{\cancel{3x^{2}+}6hx+3h^{2}+\cancel{x+}h\cancel{+1-3x^{2}+x+1}}{h}\\
 & =\lim_{h\to0}\frac{6hx+3h^{2}+h}{h}\\
 & =\lim_{h\to0}6x+3h+1\\
\boldsymbol{f'\left(x\right)} & \boldsymbol{=6x+1}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\sqrt{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(h\right)}{h}\\
 & =\lim_{h\to0}\frac{\sqrt{x+h}-\sqrt{x}}{h}\\
 & =\lim_{h\to0}\frac{\sqrt{x+h}-\sqrt{x}}{h}\cdot\frac{\sqrt{x+h}+\sqrt{x}}{\sqrt{x+h}+\sqrt{x}}\\
 & =\lim_{h\to0}\frac{x+h-x}{h\left(\sqrt{x+h}+\sqrt{x}\right)}\\
 & =\lim_{h\to0}\frac{h}{h\left(\sqrt{x+h}+\sqrt{x}\right)}\\
 & =\lim_{h\to0}\left(\frac{1}{\sqrt{x+h}+\sqrt{x}}\right)\\
f'\left(x\right) & =\frac{1}{2\sqrt{x}}
\end{align*}

\end_inset


\end_layout

