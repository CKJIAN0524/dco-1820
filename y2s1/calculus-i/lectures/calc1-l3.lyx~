\end_layout

\begin_layout Enumerate
Evaluate the derivatives
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=x\ln x$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\dx\left(x\right)\cdot\left(\ln x\right)+x\cdot\dx\left(\ln x\right)\\
 & =\ln\left(x\right)+x\cdot\frac{1}{x}\\
\boldsymbol{\dx} & \boldsymbol{=\ln\left(x\right)+1}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=2x^{2}\sin x$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =\dx\left(2x^{2}\right)\cdot\left(\sin x\right)+2x^{2}\cdot\dx\left(\sin x\right)\\
 & =4x\cdot\sin\left(x\right)+2x^{2}\cdot\left(\cos\left(x\right)\right)\\
 & =4x\sin\left(x\right)+2x^{2}\cos\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\left(2x^{3}+1\right)\left(x-5\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\left(2x^{3}+1\right)\left(x-5\right)\\
 & =\dx\left(2x^{3}+1\right)\left(x-5\right)+\left(2x^{3}+1\right)\dx\left(x-5\right)\\
 & =6x^{2}\left(x-5\right)+\left(2x^{3}+1\right)1\\
 & =6x^{3}-30x^{2}+2x^{3}+1\\
\dx & =8x^{3}-30x^{2}+1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=e^{x}\left(x^{2}+7\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\dx\left(e^{x}\right)\cdot\left(x^{2}+7\right)+e^{x}\cdot\dx\left(x^{2}+7\right)\\
 & =e^{x}\cdot\left(x^{2}+7\right)+e^{x}\cdot2x\\
 & =e^{x}\left(x^{2}+7\right)+2xe^{x}\\
 & =e^{x}\left(x^{2}+2x+7\right)
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Section
Proof & Examples
\end_layout

\begin_layout Subsection
Proofs
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\tan x\right)=\sec^{2}\left(x\right)$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\tan x\right) & =\frac{d}{dx}\left(\frac{\sin\left(x\right)}{\cos\left(x\right)}\right)\\
 & =\frac{\cos\left(x\right)\frac{d}{dx}\left(\sin x\right)-\sin\left(x\right)\frac{d}{dx}\left(\cos\left(x\right)\right)}{\left(cosx\right)^{2}}\\
 & =\frac{\cos\left(x\right)\cdot\left(\cos\left(x\right)\right)-\sin\left(x\right)\cdot\left(-\sin\left(x\right)\right)}{cos^{2}x}\\
 & =\frac{\cos^{2}x+\sin^{2}\left(x\right)}{\cos^{2}x};\text{note: \ensuremath{sin^{2}\left(x\right)+cos^{2}\left(x\right)=1}}\\
 & =\frac{1}{\cos^{2}x}\\
 & =\sec^{2}x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\cot x\right)=-csc^{2}\left(x\right)$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\cot x\right) & =\frac{d}{dx}\left(\tan^{-1}\left(x\right)\right)\\
 & =\frac{d}{dx}\left(\frac{cos\left(x\right)}{sin\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{cos\left(x\right)}{sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(cos\left(x\right)\right)-\cos\left(x\right)\frac{d}{dx}\left(\sin\left(x\right)\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{\sin\left(x\right)\left(-\sin\left(x\right)\right)-\cos\left(x\right)\left(\cos\left(x\right)\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{-\left(\sin^{2}\left(x\right)\right)-cos^{2}\left(x\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{-\left(sin^{2}\left(x\right)+cos^{2}\left(x\right)\right)}{sin^{2}\left(x\right)}\\
 & =-\frac{1}{sin^{2}\left(x\right)}\\
 & =-\csc^{2}\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\sec x\right)=\sec x\tan x$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\sec x\right) & =\frac{d}{dx}\left(\frac{1}{cos\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{1}{cos\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{\tan\left(x\right)}{\sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(\tan\left(x\right)\right)-\tan\left(x\right)\frac{d}{dx}\left(\sin\left(x\right)\right)}{sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\tan\left(x\right)\left(\cos\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\frac{\sin\left(x\right)}{\cos\left(x\right)}\left(\cos\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\sin\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)-1\right)}{\sin^{2}\left(x\right)}\\
 & =\left(\sec^{2}\left(x\right)-1\right)\cdot\left(\frac{1}{sin\left(x\right)}\right)\\
 & =\left(\tan^{2}\left(x\right)\right)\cdot\left(\frac{1}{\sin\left(x\right)}\right)\\
 & =\tan\left(x\right)\cdot\frac{\cancel{\sin\left(x\right)}}{\cos\left(x\right)}\cdot\frac{1}{\cancel{\sin\left(x\right)}}\\
 & =\tan\left(x\right)\cdot\frac{1}{cos\left(x\right)}\\
 & =\tan\left(x\right)\sec\left(x\right)\\
\boldsymbol{\frac{d}{dx}\left(\sec x\right)} & \boldsymbol{=\sec\left(x\right)\tan\left(x\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\csc\left(x\right)\right)=-\csc x\cot x$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\csc\left(x\right)\right) & =\frac{d}{dx}\left(\frac{1}{\sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(1\right)-\frac{d}{dx}\left(\sin\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(0\right)-\cos\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{-\cos\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =-\csc\left(x\right)\cot\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Example
\end_layout

\begin_layout Enumerate
Evaluate the following derivatives
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=\frac{e^{x}}{\sin\left(x\right)}$
\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{\sin\left(x\right)\cdot\frac{d}{dx}\left(e^{x}\right)-e^{x}\frac{d}{dx}\left(\sin\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\cdot e^{x}-e^{x}\cos\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =e^{x}\frac{\left(\sin\left(x\right)-\cos\left(x\right)\right)}{\sin^{2}\left(x\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{x}{\ln x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{\ln\left(x\right)\frac{d}{dx}\left(x\right)-x\frac{d}{dx}\left(\ln x\right)}{\left(\ln x\right)^{2}}\\
 & =\frac{\ln\left(x\right)-x\frac{1}{x}}{\left(\ln x\right)^{2}}\\
 & =\frac{\ln\left(x\right)-1}{\left(\ln x\right)^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{2x}{\left(x+2\right)^{2}}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\frac{\left(x+2\right)^{2}\frac{d}{dx}\left(2x\right)-2x\cdot\frac{d}{dx}\left(x+2\right)^{2}}{\left(x+2\right)^{4}}\\
 & =\frac{2\left(x+2\right)^{2}-2x\cdot2\left(x+2\right)\left(1\right)}{\left(x+2\right)^{4}}\\
 & =\frac{2\left(x+2\right)^{2}-4x\left(x+2\right)}{\left(x+2\right)^{4}}\\
 & =\frac{2\left(x+2\right)\left(\left(x+2\right)-2x\right)}{\left(x+2\right)^{4}}\\
 & =\frac{2\left(2-x\right)}{\left(x+2\right)^{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\sec x$
\end_inset


\begin_inset Formula 
\[
\dx=\sec x\tan x
\]

\end_inset


\end_layout

\end_deeper
