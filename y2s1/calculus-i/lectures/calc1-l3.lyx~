\begin_inset Formula $y=\frac{1}{3x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =\frac{1}{3}\dx\left(x^{-1}\right)\\
 & =-\frac{1}{3}x^{-2}\\
 & =-\frac{1}{3x^{2}}
\end{align*}

\end_inset


\end_layout

